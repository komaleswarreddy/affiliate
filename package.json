{
  "name": "affiliate-management-platform",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite",
    "start:server": "cross-env NODE_ENV=development tsx src/server/index.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:generate:sqlite": "drizzle-kit generate:sqlite",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:sqlite": "tsx src/server/db/migrate-sqlite.ts",
    "db:seed": "cross-env NODE_ENV=development tsx src/server/db/seed.ts",
    "db:seed:manual": "cross-env NODE_ENV=development tsx src/server/db/manual-seed.ts",
    "db:setup": "npm run db:migrate:sqlite && npm run db:seed:manual",
    "setup-db": "node setup-db.js",
    "setup-postgres": "node setup-postgres.js",
    "migrate-to-postgres": "node migrate-to-postgres.js",
    "postgres:full-setup": "npm run db:generate && npm run setup-postgres",
    "db:migrate-all": "npm run migrate-to-postgres && npm run setup-postgres",
    "dev:all": "concurrently \"npm run start:server\" \"npm run dev\"",
    "db:reset": "ts-node src/server/db/reset-sqlite.ts",
    "diagnostics": "node backend-diagnostics.js",
    "test": "vitest",
    "server:dev": "nodemon src/server/server.ts --ignore src/client/",
    "server:build": "tsc -p tsconfig.server.json",
    "server:start": "node dist/server/server.js",
    "schema:generate": "node scripts/generate-schema.js",
    "schema:migrate": "node scripts/migrate-schema.js",
    "schema:reset": "node scripts/reset-schema.js",
    "db:studio": "drizzle-kit studio",
    "setup-sqlite": "node setup-sqlite.js"
  },
